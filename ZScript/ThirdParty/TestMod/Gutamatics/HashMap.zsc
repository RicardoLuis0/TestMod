class TMGM_Hasher abstract {
	abstract virtualscope uint hash(Object o);
	abstract virtualscope bool eq(Object o0, Object o1);
	static void hashCombine(out uint seed, uint other) {
		seed ^= other + 0x9E3779B9 + (seed << 6) + (seed >> 2);
	}
}

class TMGM_BoxedInt {
	int val;
	static TMGM_BoxedInt create(int val) {
		let ret = new();
		ret.val = val;
		return ret;
	}
	TMGM_BoxedInt clone() {
		let ret = new();
		ret.val = self.val;
		return ret;
	}
}
class TMGM_IntHasher : TMGM_Hasher {
	override uint hash(Object o) { return TMGM_BoxedInt(o).val; }
	override bool eq(Object o0, Object o1) { return TMGM_BoxedInt(o0).val == TMGM_BoxedInt(o1).val; }
}

