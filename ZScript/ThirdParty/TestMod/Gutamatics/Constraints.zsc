}

enum TMGM_SymbolKind {
	TMGM_SymbolKind_Invalid,
	TMGM_SymbolKind_External,
	TMGM_SymbolKind_Slack,
	TMGM_SymbolKind_Error,
	TMGM_SymbolKind_Dummy,
}

class TMGM_Symbol {
	uint id;
	TMGM_SymbolKind kind;

	static TMGM_Symbol invalid() {
		let ret = new();
		ret.id = 0;
		ret.kind = TMGM_SymbolKind_Invalid;
		return ret;
	}

	static TMGM_Symbol create(uint id, TMGM_SymbolKind kind) {
		let ret = new();
		ret.id = id;
		ret.kind = kind;
		return ret;
	}

	TMGM_Symbol clone() {
		let ret = new();
		ret.id = self.id;
		ret.kind = self.kind;
		return ret;
	}

	String repr() {
		let ret = "Symbol(";
		ret.appendFormat("%d, ", self.id);
		switch (self.kind) {
			case TMGM_SymbolKind_Invalid:  ret.appendFormat("Invalid"); break;
			case TMGM_SymbolKind_External: ret.appendFormat("External"); break;
			case TMGM_SymbolKind_Slack:    ret.appendFormat("Slack"); break;
			case TMGM_SymbolKind_Error:    ret.appendFormat("Error"); break;
			case TMGM_SymbolKind_Dummy:    ret.appendFormat("Dummy"); break;
		}
		ret.appendFormat(")");
		return ret;
	}
}

class TMGM_SymbolHasher : TMGM_Hasher {
	override uint hash(Object o) {
		let o = TMGM_Symbol(o);
		let seed = o.id;
		hashCombine(seed, o.kind);
		return seed;
	}
	override bool eq(Object o0, Object o1) {
		let o0 = TMGM_Symbol(o0);
		let o1 = TMGM_Symbol(o1);

		return
			o0.id == o1.id &&
			o0.kind == o1.kind;
	}
}

class TMGM_Row {
	TMGM_HashMap/*<Symbol, BoxedDouble>*/ cells;
	double constant;

	static TMGM_Row create(double constant) {
		let ret = new();
		ret.cells = TMGM_HashMap.create(new("TMGM_SymbolHasher"));
		ret.constant = constant;
		return ret;
	}

	TMGM_Row clone() {
		let ret = new();
		ret.cells = TMGM_HashMap.create(new("TMGM_SymbolHasher"));
		let iter = self.cells.iter();
		while (true) {
			Object ko, vo;
			[ko, vo] = iter.next();
			if (ko is "TMGM_HashUnoccupied") { break; }

			let k = TMGM_Symbol(ko);
			let v = TMGM_BoxedDouble(vo);

			ret.cells.insert(k.clone(), v.clone());
		}
		ret.constant = self.constant;
		return ret;
	}

	double add(double v) {
		self.constant += v;
		return self.constant;
	}

	void insertSymbol(TMGM_Symbol s, double coefficient) {
		let g = self.cells.get(s);
		if (g is "TMGM_HashUnoccupied") {
			if (!TMGM_GlobalMaths.nearZero(coefficient)) {
				self.cells.insert(s, TMGM_BoxedDouble.create(coefficient));
			}
		} else {
			let g = TMGM_BoxedDouble(g);
			g.val += coefficient;
			if (TMGM_GlobalMaths.nearZero(g.val)) {
				self.cells.remove(s);
			}
		}
	}

	bool insertRow(TMGM_Row other, double coefficient) {
		let constantDiff = other.constant * coefficient;
		self.constant += constantDiff;
		let iter = other.cells.iter();
		while (true) {
			Object ko, vo;
			[ko, vo] = iter.next();
			if (ko is "TMGM_HashUnoccupied") { break; }

			let s = TMGM_Symbol(ko);
			let v = TMGM_BoxedDouble(vo).val;

			self.insertSymbol(s, v * coefficient);
		}
		return constantDiff != 0.0;
	}

	void remove(TMGM_Symbol s) {
		self.cells.remove(s);
	}

	void reverseSign() {
		self.constant = -self.constant;
		let iter = self.cells.iter();
		while (true) {
			Object ko, vo;
			[ko, vo] = iter.next();
			if (ko is "TMGM_HashUnoccupied") { break; }
			let v = TMGM_BoxedDouble(vo);
			v.val = -v.val;
		}
	}

	void solveForSymbol(TMGM_Symbol s) {
		let e = TMGM_BoxedDouble(self.cells.remove(s)).val;
		let coeff = -1.0 / e;
		self.constant *= coeff;
		let iter = self.cells.iter();
		while (true) {
			Object ko, vo;
			[ko, vo] = iter.next();
			if (ko is "TMGM_HashUnoccupied") { break; }
			let v = TMGM_BoxedDouble(vo);
			v.val *= coeff;
		}
	}

	void solveForSymbols(TMGM_Symbol lhs, TMGM_Symbol rhs) {
		self.insertSymbol(lhs, -1.0);
		self.solveForSymbol(rhs);
	}

	double coefficientFor(TMGM_Symbol s) {
		let v = self.cells.get(s);
		return (!(v is "TMGM_HashUnoccupied")) ? TMGM_BoxedDouble(v).val : 0.0;
	}

	bool substitute(TMGM_Symbol s, TMGM_Row row) {
		let e = self.cells.remove(s);
		if (!(e is "TMGM_HashUnoccupied")) {
			let coeff = TMGM_BoxedDouble(e).val;
			return self.insertRow(row, coeff);
		} else {
			return false;
		}
	}
}

