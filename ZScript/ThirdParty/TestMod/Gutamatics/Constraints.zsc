class TMGM_Variable {
	private uint id;

	static TMGM_Variable internalCreate(uint curId) {
		let ret = new();
		ret.id = curId;
		return ret;
	}

	uint getId() { return self.id; }
}
class TMGM_VariableHasher : TMGM_Hasher {
	override uint hash(Object o) { return TMGM_Variable(o).getId(); }
	override bool eq(Object o0, Object o1) {
		return TMGM_Variable(o0).getId() == TMGM_Variable(o1).getId();
	}
}

struct TMGM_VariableManager {
	private uint curId;

	TMGM_Variable createVariable() {
		let ret = TMGM_Variable.internalCreate(self.curId);
		self.curId += 1;
		return ret;
	}
}

class TMGM_Term {
	TMGM_Variable variable;
	double coefficient;

	static TMGM_Term create(TMGM_Variable variable, double coefficient) {
		let ret = new();
		ret.variable = variable;
		ret.coefficient = coefficient;
		return ret;
	}
}

class TMGM_Expr {
	Array<TMGM_Term> terms;
	double constant;

	static TMGM_Expr fromConstant(double v) {
		let ret = new();
		ret.constant = v;
		return ret;
	}
	static TMGM_Expr c(double v) { return TMGM_Expr.fromConstant(v); }

	static TMGM_Expr fromTerm(TMGM_Term term) {
		let ret = new();
		ret.terms.push(term);
		return ret;
	}

	static TMGM_Expr fromVariable(TMGM_Variable variable) {
		let ret = new();
		ret.terms.push(TMGM_Term.create(variable, 1.0));
		return ret;
	}
	static TMGM_Expr v(TMGM_Variable variable) { return TMGM_Expr.fromVariable(variable); }

	static TMGM_Expr create() {
		let ret = new();
		return ret;
	}

	void negate() {
		self.constant = -self.constant;
		for (let i = 0; i < self.terms.size(); i++) {
			let t = self.terms[i];
			t.coefficient = -t.coefficient;
		}
	}

	TMGM_Expr addAssignVariable(TMGM_Variable variable, double coefficient = 1.0) {
		self.terms.push(TMGM_Term.create(variable, coefficient));
		return self;
	}
	TMGM_Expr addAssignConstant(double constant) {
		self.constant += constant;
		return self;
	}
	TMGM_Expr addAssignExpression(TMGM_Expr e) {
		for (let i = 0; i < e.terms.size(); i++) {
			self.terms.push(e.terms[i]);
		}
		self.constant += e.constant;
		return self;
	}
	TMGM_Expr mulAssign(double coeff) {
		for (let i = 0; i < self.terms.size(); i++) {
			self.terms[i].coefficient *= coeff;
		}
		self.constant *= coeff;
		return self;
	}
}

struct TMGM_Strength {
	static double create(double a, double b, double c, double w) {
		return
			clamp(a * w, 0.0, 1000.0) * 1000000.0 +
			clamp(b * w, 0.0, 1000.0) * 1000.0 +
			clamp(c * w, 0.0, 1000.0);
	}

	const REQUIRED = 1001001000.0;
	const STRONG = 1000000.0;
	const MEDIUM = 1000.0;
	const WEAK = 1.0;

	static double clip(double s) {
		return clamp(s, 0.0, REQUIRED);
	}
}

enum TMGM_Rel {
	TMGM_Rel_LEq,
	TMGM_Rel_Eq,
	TMGM_Rel_GEq,
}

struct TMGM_ConstraintManager {
	private uint curId;

	TMGM_Constraint createConstraint(TMGM_Expr e, TMGM_Rel op, double strength) {
		let ret = TMGM_Constraint.internalCreate(e, op, strength, self.curId);
		self.curId += 1;
		return ret;
	}
}

class TMGM_Constraint {
	private TMGM_Expr expression;
	private double strength;
	private TMGM_Rel op;
	private uint id;

	static TMGM_Constraint internalCreate(TMGM_Expr e, TMGM_Rel op, double strength, uint curId) {
		let ret = new();
		ret.expression = e;
		ret.op = op;
		ret.strength = strength;
		ret.id = curId;
		return ret;
	}

	TMGM_Expr getExpr() { return self.expression; }
	TMGM_Rel getOp() { return self.op; }
	double getStrength() { return self.strength; }
	uint getId() { return self.id; }
}
class TMGM_ConstraintHasher : TMGM_Hasher {
	override uint hash(Object o) { return TMGM_Constraint(o).getId(); }
	override bool eq(Object o0, Object o1) {
		return TMGM_Constraint(o0).getId() == TMGM_Constraint(o1).getId();
	}
}

class TMGM_ConstraintBuilderWithRhs {
	TMGM_Expr lhs;
	TMGM_Expr rhs;
